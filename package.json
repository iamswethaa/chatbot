{
  "name": "chatbot",
  "private": true,
  "type": "module",
  "version": "0.0.1",
  "description": "AI Assistant",
  "author": "Swetha E",
  "license": "MIT",
  "main": "./dist-electron/main.js",
  "scripts": {
    "dev:react": "vite",
    "transpile": "tsc --project ./src/Electron/tsconfig.json && tsc --project ./src/Electron/tsconfig.preload.json",
    "dev:electron": "npm run transpile && electron .",
    "build:react": "vite build",
    "build:electron": "tsc -b",
    "build": "npm run build:react && npm run build:electron",
    "build:main": "tsc",
    "build:preload": "tsc -p src/Electron/tsconfig.preload.json",
    "build:all": "npm run build:main && npm run build:preload",
    "dist": "npm run build:all && electron-builder",
    "lint": "eslint .",
    "preview": "vite preview",
    "electron-dev": "concurrently \"npm:dev:react\" \"wait-on http://localhost:5173 && electron .\"",
    "package-win": "npm run build && electron-builder --win --x64",
    "package-mac": "npm run build && electron-builder --mac --x64",
    "package-linux": "npm run build && electron-builder --linux --x64",
    "process-docs": "node scripts/process-documents.cjs",
    "clear-db": "node scripts/clear-database.cjs"
  },
  "dependencies": {
    "@pinecone-database/pinecone": "^6.1.2",
    "@tailwindcss/vite": "^4.1.3",
    "@types/uuid": "^10.0.0",
    "@xenova/transformers": "^2.17.2",
    "axios": "^1.12.2",
    "cors": "^2.8.5",
    "dotenv": "^17.2.2",
    "express": "^5.1.0",
    "groq-sdk": "^0.32.0",
    "multer": "^2.0.2",
    "pdf-parse": "^2.1.7",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-icons": "^5.5.0",
    "react-toastify": "^11.0.5",
    "tailwindcss": "^4.1.3",
    "uuid": "^13.0.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.21.0",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/react": "^19.1.10",
    "@types/react-dom": "^19.1.7",
    "@vitejs/plugin-react": "^4.3.4",
    "concurrently": "^9.1.2",
    "electron": "^36.8.0",
    "electron-builder": "^26.0.12",
    "electron-packager": "^17.1.2",
    "eslint": "^9.21.0",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^15.15.0",
    "typescript": "~5.7.2",
    "typescript-eslint": "^8.24.1",
    "vite": "^6.2.0",
    "wait-on": "^8.0.3"
  },
  "build": {
    "appId": "chatbot",
    "productName": "ChatBot",
    "files": [
      "dist/",
      "dist-electron/",
      "build/",
      "preload.js"
    ],
    "directories": {
      "output": "release"
    },
    "win": {
      "target": "nsis",
      "icon": "build/icon.ico"
    },
    "nsis": {
      "oneClick": false,
      "allowToChangeInstallationDirectory": true
    }
  }
}
